Exercise_145.rkt:(define (sorted>? l)
Exercise_145.rkt:      (sorted>? (rest l))
Exercise_145.rkt:(check-expect (sorted>? (cons 1 '())) #true)
Exercise_145.rkt:(check-expect (sorted>? (cons 5 (cons 4 (cons 3 (cons 2 (cons 1 '())))))) #true)
Exercise_145.rkt:(check-expect (sorted>? (cons 1 (cons 2 (cons 3 (cons 4 (cons 5 '())))))) #false)
