Exercise_292.rkt:(define (sorted cmp)
Exercise_292.rkt:; determines whether l is sorted according to cmp
Exercise_292.rkt:(define (sorted? cmp l)
Exercise_292.rkt:       (sorted? cmp (rest l)))))
Exercise_292.rkt:(check-expect (sorted? < '(1 2 3)) #true)
Exercise_292.rkt:(check-expect (sorted? < '(2 1 3)) #false)
